"Generate co's"

include "cross.cat"

let invrf = rf^-1
let cobase = co0 | (IW * TLBI)
with co from cross(co_locs (cobase,inv-classes (W|TLBI)))

(* From now, co is a coherence order *)
let coi = co & int
let coe = co \ coi

(* Compute fr *)
let fr = (invrf ; co) \ id
let fri = fr & int
let fre = fr \ fri

show co,fr
